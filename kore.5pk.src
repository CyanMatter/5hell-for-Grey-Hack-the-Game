			if not localmachine.is_network_active then
				print("No network detected.")
				return 1
			end if
			router = get_router( ip )
			if not router then return "air: router not found"
			clist = router.devices_lan_ip
			if DEBUG then print("clist: "+clist)
			output = []
			for c in clist
				rw = get_router(c)
